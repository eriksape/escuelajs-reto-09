{
    "name": "platzi-reto09",
    "version": 2,
    "builds": [{
        "src": "src/index.js",
        "use": "@now/node"
    }],
    "routes": [{
        "src": "/(.*)",
        "dest": "/src/index.js"
    }],
    "env": {
        "DB_USER": "@platzireto09-db-user",
        "DB_PASSWORD": "@platzireto09-db-password",
        "DB_HOST": "@platzireto09-db-host",
        "DB_NAME": "@platzireto09-db-name"
    }
}